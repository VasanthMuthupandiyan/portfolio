{
  "name": "physio-rehab-clinic-portfolio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Professional portfolio website for PHYSIO REHAB CLINIC - Expert physiotherapy and rehabilitation services",
  "keywords": [
    "physiotherapy",
    "rehabilitation",
    "healthcare",
    "portfolio",
    "react",
    "vite"
  ],
  "homepage": "https://physiotheraphyandrehabilitation.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/VasanthMuthupandiyan/portfolio.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:github-pages": "cross-env VITE_BASE_URL=/portfolio/ NODE_ENV=production vite build",
    "build:custom-domain": "cross-env VITE_BASE_URL=/ NODE_ENV=production vite build",
    "preview": "vite preview",
    "deploy": "./deploy.sh",
    "deploy:pages": "./deploy.sh github-pages",
    "deploy:root": "./deploy.sh custom-domain",
    "deploy:local": "./deploy.sh local",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "build:analyze": "vite build --mode analyze",
    "audit:fix": "npm audit fix",
    "update-browserslist": "npx update-browserslist-db@latest",
    "troubleshoot": "npm run type-check && npm run lint && npm run clean && npm run build"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "framer-motion": "^11.5.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "cross-env": "^10.1.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.35",
    "rimraf": "^6.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.8"
  }
}
